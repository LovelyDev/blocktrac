{
  "by_result" : {
    "name" : "By Result",
    "jsonpath" : "$..[?(@.TransactionResult == 'PARAM_1')]",
    "params" : ["Result"]
  },

  "on_success" : {
    "name" : "Only Successful",
    "jsonpath" : "$..[?(@.TransactionResult == 'tesSUCCESS')]",
    "params" : []
  },


  "payment_range" : {
    "name" : "XRP Payment Range",
    "jsonpath" : "$..[?(parseInt(@.Amount) > PARAM_1 && parseInt(@.Amount) < PARAM_2)]",
    "params" : ["Min Amount", "Max Amount"]
  }
}
